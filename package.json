{
  "private": true,
  "name": "toCfe",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "cross-env dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "clean": "lerna clean --yes",
    "begin": "find . -maxdepth 3 -name node_modules -type d -exec rm -rf {} + && cnpm install && lerna bootstrap && npm run build",
    "lint-staged:js": "eslint --no-ignore --ext .js,.jsx,.ts,.tsx",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./packages"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "npm run lint:fix",
      "stylelint --syntax less"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "moment": "^2.29.1",
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.10.2",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.5",
    "dumi": "^1.1.7",
    "eslint": "^8.15.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.1",
    "lerna": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
